{
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "Node",
		"lib": ["es6"],
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"emitDeclarationOnly": false,
		"baseUrl": ".",
		"outDir": "dist/cjs",
		"paths": {"avo2-types": ["."]}
	}
}
